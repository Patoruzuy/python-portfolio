[run]
source = .
omit = 
    */tests/*
    */venv/*
    */.venv/*
    */env/*
    */instance/*
    */scripts/*
    */__pycache__/*
    */site-packages/*
    test_*.py
    migrate_to_new_schema.py
    celery_config.py
    debug_test.py
    tasks/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
